{
  "name": "subway-trips",
  "version": "1.0.0",
  "scripts": {
    "preinstall": "npm install nodemon -g",
    "start": "npm run server",
    "client": "cd client && npm start",
    "server": "cd server && npm start",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  },
  "dependencies": {}
}
